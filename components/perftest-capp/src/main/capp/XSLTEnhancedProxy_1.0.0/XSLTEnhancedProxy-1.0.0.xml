<proxy name="XSLTEnhancedProxy" startOnLoad="true" transports="https http" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <endpoint key="RealService"/>
        <inSequence>
            <fastXSLT key="fastxslt_transform_reverse"/>
        </inSequence>
        <outSequence>
            <fastXSLT key="fastxslt_transform"/>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
    <publishWSDL key="proxy_wsdl"/>
</proxy>
